        "Kiểm",
        "Kool",
        "lệch",
        "lệnh",
        "NHAN",
        "QUAN",
        "Sharingan",
        "THUAT",
        "THUNG",
        "thường",
        "TIEU",
        "trang",
        "trong",
        "Utlis"
    ],
    // ignore check file sql
    "files.exclude": {
        "**/*.sql": true
    }
}
